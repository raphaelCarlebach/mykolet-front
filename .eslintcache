ure = (res) => {\r\n        console.log(res);\r\n    }\r\n\r\n    render() { \r\n        return (\r\n            <React.Fragment>\r\n                <div className=\"container\">\r\n                    <div className=\"col-6 m-auto\">\r\n                        <h1>Login</h1>\r\n                        <form>\r\n                            <p>\r\n                                <GoogleLogin\r\n                                    clientId='51127683344-ioaf2jho3rii76knir7drg5lr203s188.apps.googleusercontent.com'\r\n                                    buttonText='Login via Google'\r\n                                    onSuccess={this.onSuccess}\r\n                                    onFailure={this.onFailure}\r\n                                />\r\n                            </p>\r\n                            <p>\r\n                                <label>Username:<br />\r\n                                    <input type=\"text\" onChange={this.handleChange} name=\"username\" id=\"username\" />\r\n                                </label>\r\n                            </p>\r\n                            <p>\r\n                                <label>Password:<br />\r\n                                    <input type=\"password\" onChange={this.handleChange} name=\"password\" id=\"password\" />\r\n                                </label>\r\n                            </p>\r\n                            <p>\r\n                                <button type=\"button\" onClick={this.handleClick} className=\"btn btn-primary\">Login</button>\r\n                            </p>\r\n                        </form>\r\n                    </div>\r\n                </div>\r\n            </React.Fragment>\r\n        );\r\n    }\r\n}\r\n \r\nexport default Login;","C:\\Users\\raphael carlebach\\Desktop\\לימודים\\REACT\\L59\\ytapp\\src\\components\\editplaylists.jsx",[],"C:\\Users\\raphael carlebach\\Desktop\\לימודים\\REACT\\L59\\ytapp\\src\\components\\header.jsx",[],"C:\\Users\\rap